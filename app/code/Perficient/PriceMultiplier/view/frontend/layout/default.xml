<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>       
        <referenceContainer name="content">
            <block class="Perficient\PriceMultiplier\Block\Utility\StoreCatalogUrl" name="store-catalog-url" template="product/utility/utility_url.phtml">
            <arguments>
                    <argument name="jsLayout" xsi:type="array">
                        <item name="components" xsi:type="array">
                            <item name="storeCatalogUrl" xsi:type="array">
                                <item name="component" xsi:type="string">Perficient_PriceMultiplier/js/utility</item>
                            </item>
                        </item>
                    </argument>
                </arguments>
            </block>           
        </referenceContainer>
    </body>
</page>
